#!/usr/bin/env bash

export PATH="$(basher package-path shellm/profiler/libexec):${PATH}"

command="$1"

case ${command} in
  "") profiler_help ;;
  *)
    command_path="$(command -v "profiler-${command}" || true)"
    if [ -z "${command_path}" ]; then
      echo "profiler: no such command '${command}'" >&2
      exit 1
    fi

    shift 1
    exec "${command_path}" "$@"
  ;;
esac
